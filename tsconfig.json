{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es2020",
    "lib": ["es2020"],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "src",
    "outDir": "dist",
    "typeRoots": ["./node_modules/@types", "./src/types"],
  },
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ],
}
